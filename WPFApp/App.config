<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="AzureLanguage" value="en"/> <!-- en, ja, pt, zh, es -->
    <add key="AzureMaxCandidate" value="1"/> <!-- valor por defecto 1, max 3-->
    <add key="AzureSubscriptionKey" value=""/>
    <add key="AzureURL" value=""/>
    <add key="MaxRequestAzure" value="20"/>

    <add key="AzureTranslateSubscriptionKey" value=""/>
    <add key="AzureTranslateURL" value=""/>
    <add key="AzureGeneralTranslateURL" value="https://api-eur.cognitive.microsofttranslator.com/translate"/>
    <add key="AzureTranslateRegion" value="westeurope"/>
    <add key="UseAzureTranslate" value="false"/>
  </appSettings>
</configuration>
